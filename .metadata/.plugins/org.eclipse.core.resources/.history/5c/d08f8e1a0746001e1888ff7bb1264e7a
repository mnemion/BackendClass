package javatest;

import java.util.Scanner;

public class java9 {
	//packge app(app1.java) 연계
	public static void main(String[] args) {
		System.out.println("단어를 입력하세요:");
		Scanner sc = new Scanner(System.in);
		
		String word = sc.nextLine();
		int ea = word.length();
		System.out.println(ea);
	}
}
