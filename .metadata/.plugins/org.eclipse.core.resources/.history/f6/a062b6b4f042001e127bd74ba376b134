package javatest;
/*
 반복문 for, while, do~while
*/
public class java6 {

	public static void main(String[] args) {
		new data3().abc();
	}
}

/* 1부터 5까지 출력문 */
class data3 {
    public void abc() {
    	/*
    	int f;
        for(f = 1; f<=5; f++) {
        	System.out.println(f);
        }
        */
    	/*
    	int w = 1;
    	while(w <= 5) {
    		System.out.println(w);
    		w++;
    	}
    	*/
    	/*
    	int w = 1;
    	do {
    		System.out.println(w);
    		w++;
    	} while(w <= 5);
    	*/
    }
}