function b_write(){
    //글쓰기
    if(document.getElementsByid("utitle")[0].value == ""){
        alert("제목을 입력해주세요.");
    }
    else if(document.getElementsByName("uname")[0].value == ""){
        alert("글쓴이를 입력해주세요.");
    }
    else if(document.getElementsByName("upass")[0].value == ""){
        alert("패스워드를 입력해주세요.");
    }
    else if(document.getElementsByName("texts")[0].value == ""){
        alert("내용을 입력해주세요.");
    }
    else {
        f.submit();
    }
}