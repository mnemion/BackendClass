package javatest;

import java.util.*;

class data1{	//외부 클래스
	Scanner sc = null;
	
	public void abc(String a, int b, String c) {
		this.sc = new Scanner(System.in);
	}
}

public class java4 {	//메인 클래스
	int a, b = 0;
	
	public void java1() {
		this.a = 20;
		this.b = 30;
		java2();	//같은 클래스에서 해당 메소드로 실행
	}
	
	public void java2() {
		int sum = this.a + this.b;
		//System.out.println(sum);
	}
}
