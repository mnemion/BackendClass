package javatest;

//random 함수 사용법

public class java14 {

	public static void main(String[] args) {
		//Math.random() : 랜덤함수 (기본 자료형 double)
		double no = Math.random() * 10;
		//int no1 = (int) Math.ceil(Math.random() * 10);
		int no1 = Integer.valueOf(0) Math.ceil(Math.random() * 10);
		System.out.println(no);
	}
}
