package javatest;
//즉시 실행 메소드 부분

public class java10 {
	
	public static void main(String[] args) {
		box1 b = new box1();
		b.box1();
	}
}

class box1{
	public box1() {	//즉시 실행 메소드
		System.out.println("즉시 실행 메소드1");
	}
	
	public box1(String a) {
		System.out.println("즉시 실행 메소드2");
	}
	
	public box1(int a) {
		System.out.println("즉시 실행 메소드2");
	}
	
	public void box1() {	//일반 메소드 구조가 다름
		System.out.println("즉시 실행 메소드2");
	}
}