package javatest;

class abc{
	String data = "";	//일반 변수선언
	static String data2 = ""; 	//메모리에 변수값을 생성시켜 놓음
	public abc() {	//즉시 실행 메소드
		
	}
	
	public void data() {	//일반 메소드
		String user = "홍길동";
		System.out.println(user);
	}
	public static void data2() {
		String user2 = "이순신";
		System.out.println(user2);
	}
	String data3() {
		return null;
	}
}