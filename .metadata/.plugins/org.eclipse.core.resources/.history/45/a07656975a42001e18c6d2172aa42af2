package javatest;

//this : 외부 class에서 사용가능
class abc{
	String data = "";	//일반 변수선언
	static String data2 = ""; 	//메모리에 변수값을 생성시켜 놓음
	
	public abc() {	//즉시 실행 메소드
		this.data = "20";
	}
	
	public void data() {	//일반 메소드
		String user = "홍길동";
		System.out.println(user);
	}
	public static void data2() {	//메모리 등록 메소드
		String user2 = "이순신";
		System.out.println(user2);
	}
	String data3() {	//리턴 메소드
		return null;
	}
}