package javatest;
//static, if문 + Scanner
public class java3 {	//메인 클래스

	public static void main(String[] args) {	//실행 메소드
		java3_data jd = new java3_data();
		jd.abc();
	}
}

class java3_data{
	int a = 10;
	static int b = 10;
	
	public void abc() {
		System.out.println(this.a);
		System.out.println(this.b);
		this.a = 20;
		this.b = 20;
		System.out.println(this.a);
		System.out.println(this.b);
	}
}